{
  "name": "estuary",
  "version": "1.0.0",
  "description": "Note Taking and To-Do App",
  "main": "app.js",
  "engines": {
    "node": "12.14.1",
    "npm": "6.13.4"
  },
  "scripts": {
    "test": "NODE_ENV=test nodemon --exec 'nyc mocha test --recursive'",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "cpustejovsky",
  "license": "MIT",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/pickers": "^3.2.10",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^5.1.0",
    "connect-flash": "^0.1.1",
    "cookie-session": "^1.4.0",
    "cpustejovsky-dyno-waker": "^1.0.4",
    "date-fns": "^2.13.0",
    "express": "^4.17.1",
    "express-sanitizer": "^1.0.5",
    "googleapis": "^39.2.0",
    "lodash": "^4.17.14",
    "mailgun-js": "^0.22.0",
    "method-override": "^3.0.0",
    "mocha": "^7.1.2",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-stub": "^1.1.1",
    "sinon": "^9.0.2"
  },
  "devDependencies": {
    "nyc": "^15.0.1"
  }
}
