{
  "name": "estuary",
  "version": "1.0.0",
  "description": "Note Taking and To-Do App",
  "main": "app.js",
  "scripts": {
    "test": "nodemon --exec 'mocha test --recursive'",
    "local": "nodemon app.js test",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "engines": {
    "node": "10.x"
  },
  "author": "cpustejovsky",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "connect-flash": "^0.1.1",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-sanitizer": "^1.0.5",
    "express-session": "^1.16.2",
    "lodash": "^4.17.14",
    "mailgun-js": "^0.22.0",
    "method-override": "^3.0.0",
    "mocha": "^6.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local-mongoose": "^5.0.1",
    "request": "^2.88.0"
  }
}
